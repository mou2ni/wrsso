<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
        <!-- <link rel="stylesheet" href="{ asset('css/warisso.css') }}">
            <link rel="stylesheet" href="{ asset('css/jquery-ui.min.css') }}">
            -->

            <link rel="stylesheet" href="{{ asset('build/css/app.css') }}">
            <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
            <link rel="stylesheet" href="{{ asset('css/styles.css') }}">
            <link rel="stylesheet" href="{{ asset('css/select2/select2.min.css') }}">

        {% endblock %}




    </head>
    <body>

    <div class="app">
        <div class="app-container">

            <div class="app-content">
                <!-- START PAGE CONTAINER -->
                <div class="container">
                    <div><span class="float-sm-right glyphicon glyphicon-user">{{ app.user }}</span><a href="{{ path('logout') }}" class="close" > <span class="glyphicon glyphicon-off" aria-hidden="true"></span>Deconnecter </a> </div>
                    {% for msg in app.session.flashBag.get('success') %}
                        <div class="alert alert-success">
                            {{ msg }}
                        </div>
                    {% endfor %}
                    {% for msg in app.session.flashBag.get('error') %}
                        <div class="alert alert-danger">
                            {{ msg }}
                        </div>
                    {% endfor %}
                    <div class="app-content-separate-content">
                        <!-- CONTENT CONTAINER -->
                        <div class="container">
                            <div class="block block-condensed padding-top-20">
                                <div class="block-content">
                                    {% block body %}

                                    {% endblock %}
                                </div>
                            </div>
                        </div>
                        <!-- CONTENT CONTAINER -->
                    </div>


                </div>

            </div>
        </div>
    </div>
    </body>
    {% block javascripts %}
        <!-- <script src="{ asset('js/jquery-3.1.1.js') }}"></script>
            <script src="{ asset('js/jquery-ui.min.js') }}"></script>
            <script src="{ asset('js/main.js') }}"></script>-->


        <script src="{{ asset('build/js/app.js') }}"></script>
        <script src="{{   asset('js/warisso.js') }}"></script>
        <script src="{{ asset('js/app.js') }}"></script>
        <script src="{{ asset('js/app_plugins.js') }}"></script>
        <script src="{{ asset('js/select2/select2.min.js') }}"></script>

    {% endblock %}




</html>
