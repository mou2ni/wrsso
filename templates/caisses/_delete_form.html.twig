<form method="post" action="{{ path('caisses_delete', {'id': caiss.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ caiss.id) }}">
    <button class="btn">Delete</button>
</form>