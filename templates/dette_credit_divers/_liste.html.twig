<table class="table">
    <thead>
    <tr>
        <th>DateDC</th>
        <th>Libelle</th>
        <th>MCredit</th>
        <th>MDette</th>
        <th>actions</th>
    </tr>
    </thead>
    <tbody>
    {% for dette_credit_diver in journeeCaisse.detteCredits %}
        <tr>

            <td>{{ dette_credit_diver.dateDC ? dette_credit_diver.dateDC|date('Y-m-d H:i:s') : '' }}</td>
            <td>{{ dette_credit_diver.libelle }}</td>
            <td>{{ dette_credit_diver.mCredit }}</td>
            <td>{{ dette_credit_diver.mDette }}</td>
            <td>
                <a href="{{ path('dette_credit_divers_show', {'id': dette_credit_diver.id}) }}">show</a>
                <a href="{{ path('dette_credit_divers_edit', {'id': dette_credit_diver.id}) }}">edit</a>
            </td>
        </tr>
    {% else %}
        <tr>
            <td colspan="5">no records found</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
