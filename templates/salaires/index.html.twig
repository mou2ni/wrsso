{% extends 'base.html.twig' %}

{% block title %}Salaires index{% endblock %}

{% block body %}
    <h1>Salaires index</h1>

    <table class="table table-striped">
        <thead>
            <tr>
                <th>Id</th>
                <th>PeriodeSalaire</th>
                <th>MNetTotal</th>
                <th>MTaxeTotal</th>
                <th>MImpotTotal</th>
                <th>MSecuriteSocialTotal</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for salaire in salaires %}
            <tr>
                <td>{{ salaire.id }}</td>
                <td>{{ salaire.periodeSalaire}}</td>
                <td>{{ salaire.mNetTotal }}</td>
                <td>{{ salaire.mTaxeTotal }}</td>
                <td>{{ salaire.mImpotTotal }}</td>
                <td>{{ salaire.mSecuriteSocialTotal }}</td>
                <td>
                    <a href="{{ path('salaires_show', {'id': salaire.id}) }}">show</a>
                    <a href="{{ path('salaires_edit', {'id': salaire.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="7">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('salaires_new') }}">Create new</a>
{% endblock %}
