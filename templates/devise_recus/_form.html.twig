{{ form_start(form) }}
    {{ form_widget(form.dateRecu) }}
    {{ form_widget(form.nom) }}
    {{ form_row(form.prenom) }}
    {{ form_row(form.typePiece) }}
    {{ form_row(form.numPiece) }}
    {{ form_row(form.expireLe) }}
    {{ form_row(form.paysPiece) }}
    {{ form_row(form.motif) }}

<h3>Devises</h3>
<ul class="deviseMouvements" data-prototype="{{ form_widget(form.deviseMouvements.vars.prototype.sens)|e('html_attr') }}
{{ form_widget(form.deviseMouvements.vars.prototype.devise)|e('html_attr') }}
    {{ form_widget(form.deviseMouvements.vars.prototype.nombre)|e('html_attr') }}
    {{ form_widget(form.deviseMouvements.vars.prototype.taux)|e('html_attr') }}
    {{ form_widget(form.deviseMouvements.vars.prototype.contreValeur)|e('html_attr') }}
    {{ form_widget(form.deviseMouvements.vars.prototype.deviseJournee )|e('html_attr') }}">
    {% for deviseMouvement in form.deviseMouvements %}
        <li>{{ form_widget(deviseMouvement.sens) }}</li>
        <li>{{ form_widget(deviseMouvement.devise) }}</li>
        <li>{{ form_widget(deviseMouvement.nombre) }}</li>
        <li>{{ form_widget(deviseMouvement.taux) }}</li>
    {% endfor %}
</ul>
{{ form_end(form) }}