<table class="table">
    <thead>
    <tr>
        <th>Client</th><th>Montant</th><th>libelle</th>
    </tr>
    </thead>
    <tbody>
    <th colspan="2">Total</th><th>{{ journeeCaisse.mDepotClient }}</th>
    {% for depot in journeeCaisse.transactions %}
        {% for trcpte in depot.transactionComptes %}
            {% if (trcpte.compte.typeCompte!='I') and (trcpte.mCredit) %}
                <tr><td>><td>{{ trcpte.compte.intitule }}</td><td>{{ trcpte.mCredit|number_format(0, '.', ' ') }}</td><td>{{ depot.libelle }}</td></tr>
            {% endif %}
        {% endfor %}
    {% endfor %}
    </tbody>
</table>

