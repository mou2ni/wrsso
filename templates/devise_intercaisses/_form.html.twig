{{ form_start(form, {'attr': {'class': 'form-vertical','onsubmit':"return confirm('Confirmer intercaisse ?');"}}) }}
<div class="form-group">
    <div class="form-inline">
        {{ form_row(form.journeeCaisseSource) }}
    </div>
</div>

<h3>Devises Ã  transferer </h3>

<div class="form-group">
    <div class="form-row">
        <div class="form-row deviseMouvements" data-prototype="
       {{ form_widget(form.deviseTmpMouvements.vars.prototype.devise)|e('html_attr') }}
    {{ form_widget(form.deviseTmpMouvements.vars.prototype.nombre)|e('html_attr') }}">
