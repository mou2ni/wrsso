            <tr>
                <td colspan="5">Aucun enregistrement trouv√©</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
    <div class="row">
        {{ include('_pagination.html.twig',{'path':'devise_mouvements_index'}) }}
    </div>
    <a href="{{ path('devise_mouvements_new') }}">Ajouter</a>
