<table class="table table-striped">
    <thead>
    <tr>
        <th>Id</th>
        <th>Libelle</th>
        <th>actions</th>
    </tr>
    </thead>
    <tbody>
    {% for system_transfert in system_transferts %}
        <tr>
            <td>{{ system_transfert.id }}</td>
            <td>{{ system_transfert.libelle }}</td>
            <td>
                <a href="{{ path('system_transfert_show', {'id': system_transfert.id}) }}">Details</a>
                <a href="{{ path('system_transfert_edit', {'id': system_transfert.id}) }}">Modifier</a>
            </td>
        </tr>
    {% else %}
        <tr>
            <td colspan="3">Auncun système trouvé</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
