
<div class="row">

    <div class="col">
        <h3>Soldes a l'ouverture</h3>

        <div class="form-inline">
            <div>{{ form_label(form.mLiquiditeOuv) }}
            </div><div>{{ form_widget(form.mLiquiditeOuv) }}</div>
        </div>
        <div class="form-inline">
            <div>{{ form_label(form.mSoldeElectOuv) }}
            </div><div>{{ form_widget(form.mSoldeElectOuv) }}</div>
        </div>
        <div class="form-inline">
            <div>{{ form_label(form.mDetteDiversOuv) }}
            </div><div>{{ form_widget(form.mDetteDiversOuv) }}</div>
        </div>
        <div class="form-inline">
            <div>{{ form_label(form.mCreditDiversOuv) }}
            </div><div>{{ form_widget(form.mCreditDiversOuv) }}</div>
        </div>
        ===================
        <div class="form-inline">
            <div>{{ form_label(form.mEcartOuv) }}
            </div><div>{{ form_widget(form.mEcartOuv) }}</div>
        </div>

        ===================
        <div >
            {% for deviseJournee in form.deviseJournees %}
                <div class="form-inline">
                    <div>{{ form_widget(deviseJournee.devise) }}
                    </div><div>{{ form_widget(deviseJournee.billetOuv) }}</div>
                </div>

            {% else %}
                Aucune devise
            {% endfor %}
        </div>

        <div class="form-inline">{{ form_widget(form.save) }}</div>
    </div>
    <div class="col">
        <h3>Soldes Actuels</h3>
        <div class="form-inline">
            <div>{{ form_label(form.mLiquiditeFerm) }}</div>
            <div>{{ form_widget(form.mLiquiditeFerm) }}</div>
            <div class="form-inline">
                {{ include('journee_caisses/_btn_action_billetage.html.twig',{'id':journeeCaisse.billetFerm.id, 'operation':'FERMER', 'devise':0,'btn_name':'Billetage', 'btn_label':'Billetage'}) }}
            </div>
        </div>
        <div class="form-inline">
            <div>{{ form_label(form.mSoldeElectFerm) }}</div>
            <div>{{ form_widget(form.mSoldeElectFerm) }}</div>
            <div class="form-inline">
                {{ include('journee_caisses/_btn_action_elect.html.twig',{'id':journeeCaisse.systemElectInventFerm.id, 'operation':'FERMER', 'btn_name':'Solde', 'btn_label':'Solde'}) }}
            </div>
        </div>
        <div class="form-inline">
            <div>{{ form_label(form.mDetteDiversFerm) }}
            </div><div>{{ form_widget(form.mDetteDiversFerm) }}</div>
            <div class="form-inline">
                {{ include('journee_caisses/_btn_action_url.html.twig',{'path':'dette_credit_divers', 'operation':'FERMER' ,'btn_name':'midifier', 'btn_label':'Modifier'}) }}
            </div>
        </div>
        <div class="form-inline">
            <div>{{ form_label(form.mCreditDiversFerm) }}
            </div><div>{{ form_widget(form.mCreditDiversFerm) }}</div>
            <div class="form-inline">
                {{ include('journee_caisses/_btn_action_url.html.twig',{'path':'dette_credit_divers' , 'operation':'FERMER','btn_name':'midifier', 'btn_label':'Modifier'}) }}
            </div>
        </div>

        <div class="form-inline">
            {{ form_label(form.mIntercaisses) }}
            {{ form_widget(form.mIntercaisses) }}
            <div class="form-inline">
                {{ include('journee_caisses/_btn_action_url.html.twig',{'path':'inter_caisses_ajout' , 'operation':'FERMER','btn_name':'midifier', 'btn_label':'Modifier'}) }}
            </div>
        </div>
        <div class="form-inline">
            {{ form_label(form.mEmissionTrans) }}
            {{ form_widget(form.mEmissionTrans) }}
            <div class="form-inline">
                {{ include('journee_caisses/_btn_action_url.html.twig',{'path':'transfert_internationaux_ajout' , 'operation':'FERMER','btn_name':'midifier', 'btn_label':'Modifier'}) }}
            </div>
        </div>
        <div class="form-inline">
            {{ form_label(form.mReceptionTrans) }}
            {{ form_widget(form.mReceptionTrans) }}
            <div class="form-inline">
                {{ include('journee_caisses/_btn_action_url.html.twig',{'path':'transfert_internationaux_ajout' , 'operation':'FERMER','btn_name':'midifier', 'btn_label':'Modifier'}) }}
            </div>
        </div>
        <div class="form-inline">
            {{ form_label(form.mCvd) }}
            {{ form_widget(form.mCvd) }}
        </div>
        <div class="form-inline">
            {{ form_label(form.mRetraitClient) }}
            {{ form_widget(form.mRetraitClient) }}
            <div class="form-inline">
                {{ include('journee_caisses/_btn_action_url.html.twig',{'path':'retrait' , 'operation':'FERMER','btn_name':'midifier', 'btn_label':'Modifier'}) }}
            </div>
        </div>
        <div class="form-inline">
            {{ form_label(form.mDepotClient) }}
            {{ form_widget(form.mDepotClient) }}
            <div class="form-inline">
                {{ include('journee_caisses/_btn_action_url.html.twig',{'path':'depot' , 'operation':'FERMER','btn_name':'midifier', 'btn_label':'Modifier'}) }}
            </div>
        </div>
        ===================
        <div class="form-inline">
            <div>{{ form_label(form.mEcartFerm) }}
            </div><div>{{ form_widget(form.mEcartFerm) }}</div>
        </div>
        ===================
        <div class="row">
            <div class="col">
                {% for deviseJournee in form.deviseJournees %}
                    <div class="form-inline">
                        <div>{{ form_widget(deviseJournee.devise) }}</div>
                        <div>{{ form_widget(deviseJournee.billetFerm) }}</div>
                    </div>

                {% else %}
                    Aucune devise
                {% endfor %}
            </div>
            <div class="col">
                {% for deviseJournee in journeeCaisse.deviseJournees %}
                    <div class="form-inline">
                        {{ include('journee_caisses/_btn_action_billetage.html.twig',{'id':deviseJournee.billetFerm.id,'operation':'FERMER', 'devise':deviseJournee.devise.id,'btn_name':'Billetage', 'btn_label':'Billetage'}) }}
                    </div>
                {% else %}
                    Aucune devise
                {% endfor %}
            </div>
        </div>

        <div hidden>{{ form_rest(form) }}</div>

    </div>
    <div class="form-inline">
        {{ include('journee_caisses/_btn_action_url.html.twig',{'path':'journee_caisses_enregistrer' , 'operation':'FERMER','btn_name':'submit', 'btn_label':'Enregistrer'}) }}
    </div>

</div>
